{
  "name": "react-custom-carousel",
  "version": "0.1.0",
  "description": "zhang's react custom carousel for https://runeapes.io/",
  "keywords": [
    "react",
    "carousel",
    "draggable",
    "infinite loop"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "scripts": {
    "dev": "start-storybook -p 6006",
    "lint": "eslint \"src/components/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/components/*.{js,jsx,ts,tsx}\"",
    "build-storybook": "build-storybook",
    "gh-deploy": "build-storybook && gh-pages -d storybook-static",
    "bundle": "rollup -c"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "babel-loader": "^8.2.5",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.78.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "storybook-css-modules": "^1.0.8",
    "styled-components": "^6.1.11"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "./dist"
  ]
}
